    <window width="1024" height="700" if="desktop" />
    <window width="0" height="0" if="html5" />
    <window orientation="landscape" fps="60" vsync="true" if="cpp"/>
	<window hardware="true" allow-shaders="true" require-shaders="true" depth-buffer="true" stencil-buffer="true" />

	<source path="src" />

	<classpath path="../away3d-core-openfl/src" />
	
	<set name="html5-backend" value="openfl-html5-dom" />
	<haxelib name="openfl" />
	
	<assets path="embeds" exclude="nme.svg" />

	<icon path="embeds/openfl.svg" />
	
	<haxedef name="source-map-content"/>
	<haxedef name="HXCPP_STACK_TRACE" />
	<haxedef name="HXCPP_STACK_LINE" />
	<haxedef name="HXCPP_DEBUG_LINK" />

	<haxeflag name="-debug" />

	<android minimum-sdk-version="7" />

